{
  "name": "qualitour-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npx tsx scripts/prefetch-zaui.js && astro build && npm run check-files",
    "check-files": "echo 'Total files in dist:' && find dist -type f | wc -l",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:staging": "npm run build && npx wrangler pages deploy dist --project-name=qualitour-astro --branch=staging",
    "pages:dev": "wrangler pages dev dist"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/kaushan-script": "^5.2.8",
    "@fontsource/material-icons": "^5.2.7",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.5",
    "embla-carousel-react": "^8.6.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "lightningcss": "^1.30.2",
    "wrangler": "^4.54.0"
  }
}